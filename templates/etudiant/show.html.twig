{% extends 'baselayout.html.twig' %}

{% block title %}Etudiant{% endblock %}

{% block body %}
    <h1>Etudiant</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ etudiant.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ etudiant.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ etudiant.prenom }}</td>
            </tr>
            <tr>
                <th>Departement</th>
                <td>{{ etudiant.departement }}</td>
            </tr>
            <tr>
                <th>Niveau</th>
                <td>{{ etudiant.niveau }}</td>
            </tr>
            <tr>
                <th>Username</th>
                <td>{{ etudiant.username }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ etudiant.password }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('etudiant_index') }}">back to list</a>

    <a href="{{ path('etudiant_edit', {'id': etudiant.id}) }}">edit</a>

    {{ include('etudiant/_delete_form.html.twig') }}
{% endblock %}
